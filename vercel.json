{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "/routes/auth.js"
    },
    {
      "src": "/api/user/(.*)",
      "dest": "/routes/user.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}